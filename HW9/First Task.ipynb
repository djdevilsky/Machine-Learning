    "def pairs(N):\n",
    "    n = len(N)\n",
    "    s = 0\n",
    "    for i in range(n):\n",
    "        for j in range(i + 1, n):\n",
    "            if j - i <= N[i] + N[j]:\n",
    "                s += 1\n",
    "    if s > 10000000:\n",
    "        s = -1\n",
    "    return s\n",
    "\n",
    "l = []\n",
    "a = input()\n",
    "for i in a.split():\n",
    "    l.append(int(i))\n",
    "\n",
    "print(pairs(l))"
   ]
